{
  "version": 2,
  "functions": [
    {
      "includeFiles": ["prisma/**"],
      "src": "dist/*.js"
    }
  ],
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node",
      "config": { "maxLambdaSize": "2048MB" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}